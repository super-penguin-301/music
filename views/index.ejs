    <section id="hero" class="d-flex align-items-center justify-content-center">
        <div class="container" data-aos="fade-up">
            <div class="row justify-content-center" data-aos="fade-up" data-aos-delay="150">
                <div class="col-xl-6 col-lg-8">
                    <h1>It’s Not Just Music. &nbsp; It’s The Universal Language<span>.</span></h1>
                    <h2>With Musicly</h2>
                </div>
            </div>

            <div class="row gy-4 mt-5 justify-content-center" data-aos="zoom-in" data-aos-delay="250">
                <div class="col-xl-2 col-md-4">
                    <a class='nav-link scrollto' href="#artists">
                        <div class="icon-box">
                            <i class="fas fa-microphone"></i>
                            <h3>Top Artists</h3>
                        </div>
                    </a>
                </div>
                <div class="col-xl-2 col-md-4">
                    <a class='nav-link scrollto' href="#topten">
                        <div class="icon-box">
                            <i class="fas fa-drum"></i>
                            <h3>Top Tracks</h3>
                            <!-- use template for songs-->
                        </div>
                    </a>
                </div>
                <div class="col-xl-2 col-md-4">
                    <a class='nav-link scrollto' href="#albums">

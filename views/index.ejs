        <section id="features" class="services">
            <div class="container" data-aos="fade-up">

                <div class="section-title">
                    <h2>OUR</h2>
                    <p>Features</p>
                </div>

                <div class="row">
                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="zoom-in" data-aos-delay="100">
                        <div class="icon-box">
                            <div class="icon"><i class="bx bxl-dribbble"></i></div>
                            <h4><a href="">Sport</a></h4>
                            <p>Enjoy with free & unlimitted number of songs during your hard exercises</p>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="zoom-in" data-aos-delay="100">
                        <div class="icon-box">
                            <div class="icon"><i class="bx bx-world"></i></div>
                            <h4><a href="">WhereEver</a></h4>
                            <p>Enjoy by listening to your favorite music anywhere and anytime you are in</p>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="zoom-in" data-aos-delay="100">
                        <div class="icon-box">
                            <div class="icon"><i class="bx bx-slideshow"></i></div>
                            <h4><a href="">Hight Quality</a></h4>
                            <p>Our tracks came with high quality and clear voice to give you a comfort</p>
                        </div>
                    </div>


                </div>

            </div>
        </section>
        <!-- End Features Section -->

        <!-- ======= Clients Section ======= -->
        <section id="clients" class="clients">
